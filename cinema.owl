        <rdfs:label xml:lang="en">Beginning of the shooting</rdfs:label>
        <rdfs:label xml:lang="fr">Date du début du tournage</rdfs:label>
        <rdfs:seeAlso rdf:resource="s:dateCreated"/>
    </owl:ObjectProperty>
    


