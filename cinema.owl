        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://schema.org/Movie"/>
        <rdfs:range rdf:resource="https://schema.org/Date"/>
        <owl:propertyDisjointWith rdf:resource="http://exemple.org/cinema#date_fin_tournage"/>
