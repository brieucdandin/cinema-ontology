    <owl:ObjectProperty rdf:about="http://exemple.org/cinema#date_film">
        <rdfs:domain rdf:resource="https://schema.org/Movie"/>
        <rdfs:range rdf:resource="https://schema.org/Date"/>
        <rdfs:comment xml:lang="fr">Propriété d&apos;un film indiquant la date d&apos;au moins un des événements du film</rdfs:comment>
        <rdfs:label xml:lang="fr">Date d&apos;un événement du film</rdfs:label>
        <rdfs:label xml:lang="en">Date of an event in the film</rdfs:label>
        <rdfs:seeAlso rdf:resource="s:contentReferenceTime"/>
    </owl:ObjectProperty>
    


