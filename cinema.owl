    <!-- http://example.org/cinema#ValuePartition -->

    <owl:Class rdf:about="http://example.org/cinema#ValuePartition">
        <rdfs:comment xml:lang="en">A ValuePartition is a pattern that describes a restricted set of classes from which a property can be associated. The parent class is used in restrictions, and the covering axiom means that only members of the subclasses may be used as values. The possible subclasses cannot be extended without updating the ValuePartition class.</rdfs:comment>
        <rdfs:label xml:lang="fr">Partition de valeurs</rdfs:label>
    </owl:Class>
    


