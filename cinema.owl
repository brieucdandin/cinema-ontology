    <!-- http://example.org/cinema#Acteur -->

    <owl:Class rdf:about="http://example.org/cinema#Acteur">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://schema.org/Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://exemple.org/cinema#acteur_dans"/>
                        <owl:someValuesFrom rdf:resource="https://schema.org/Movie"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://example.org/cinema#Profession"/>
        <rdfs:label xml:lang="fr">Acteur</rdfs:label>
        <rdfs:label xml:lang="en">Actor</rdfs:label>
    </owl:Class>
    


    <!-- http://example.org/cinema#Appreciation -->

    <owl:Class rdf:about="http://example.org/cinema#Appreciation">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://example.org/cinema#Bon"/>
                    <rdf:Description rdf:about="http://example.org/cinema#Excellent"/>
                    <rdf:Description rdf:about="http://example.org/cinema#Mauvais"/>
                    <rdf:Description rdf:about="http://example.org/cinema#Moyen"/>
                    <rdf:Description rdf:about="http://example.org/cinema#Nul"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://example.org/cinema#ValuePartition"/>
        <rdfs:label xml:lang="fr">Appréciation</rdfs:label>
        <rdfs:label xml:lang="en">Rating</rdfs:label>
    </owl:Class>
    


